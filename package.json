{
  "name": "obsidian-steward",
  "version": "2.0.4",
  "description": "This is a sample plugin for Obsidian (https://obsidian.md)",
  "main": "main.js",
  "scripts": {
    "dev": "npm run dev:css & node esbuild.config.mjs",
    "dev:css": "postcss src/styles.css -o styles.css --watch",
    "build": "tsc -noEmit -skipLibCheck && npm run build:css && node esbuild.config.mjs production",
    "build:css": "postcss src/styles.css -o styles.css",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest",
    "test:watch": "jest --watch",
    "postinstall": "patch-package"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@codemirror/autocomplete": "github:codemirror/autocomplete",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/crypto-js": "^4.2.2",
    "@types/jest": "^29.5.12",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "autoprefixer": "^10.4.23",
    "builtin-modules": "3.3.0",
    "esbuild": "0.17.3",
    "jest": "^29.7.0",
    "js-yaml": "^4.1.1",
    "obsidian": "latest",
    "patch-package": "^8.0.1",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "prettier": "^3.5.3",
    "tailwindcss": "^4.1.18",
    "ts-jest": "^29.1.2",
    "tslib": "2.4.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.2",
    "@ai-sdk/deepseek": "^2.0.2",
    "@ai-sdk/elevenlabs": "^2.0.6",
    "@ai-sdk/google": "^3.0.2",
    "@ai-sdk/groq": "^3.0.2",
    "@ai-sdk/hume": "^2.0.6",
    "@ai-sdk/openai": "^3.0.2",
    "@ai-sdk/openai-compatible": "^2.0.2",
    "ai": "^6.0.5",
    "crypto-js": "^4.2.0",
    "dexie": "^4.0.11",
    "i18next": "^21.10.0",
    "jsonrepair": "^3.13.0",
    "ollama-ai-provider-v2": "^1.5.5",
    "zod": "^4.1.8"
  }
}
